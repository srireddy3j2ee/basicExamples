package firstProject;

public class ifElse {

	public static void main(String[] args) {
		int givenNum=20;

		if (givenNum<0)
			System.out.println("It is a negative number");
		else
		{
			if(givenNum>10)
				System.out.println("It is greater than 10");
			else
				System.out.println("Given number is "+ givenNum);
		}
	}
}
